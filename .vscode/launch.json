{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "üü¢ Run actrun",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "buildFlags": "-tags=github_impl",
      "hideSystemGoroutines": true,
      "env": {
        "CGO_ENABLED": "0"
      },
      "args": [
        "--env_file=${workspaceFolder}/.env",
      ]
    },
    {
      "name": "‚úÖ Validate action graph",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "hideSystemGoroutines": true,
      "args": [
        "validate",
        "~/Desktop/test.act"
      ],
      "env": {
        "CGO_ENABLED": "0"
      },
    },
    {
      "name": "üß™ Run Current Test File",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${file}",
      "buildFlags": "-tags=github_impl,tests_system",
      "args": [],
      "showLog": true,
      "env": {
        "CGO_ENABLED": "0",
      },
    },
    {
      "name": "‚öôÔ∏è Generate node stubs",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "buildFlags": "-tags=dev,api,cpython",
      "args": [
        "dev",
        "generate_stubs"
      ],
      "env": {
        "CGO_ENABLED": "1",
      },
      "hideSystemGoroutines": true,
    },
    {
      "name": "‚ÜóÔ∏è Update Database",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "console": "integratedTerminal",
      "program": "${workspaceFolder}",
      "buildFlags": "-tags=github_impl,dev,api,cpython",
      "args": [
        "--env_file=.env",
        "dev",
        "update",
        "database",
      ],
      "hideSystemGoroutines": true,
    },
    {
      "name": "üìñ Update Docs",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "buildFlags": "-tags=github_impl,dev",
      "args": [
        "--env_file=.env",
        "dev",
        "update",
        "docs"
      ],
      "env": {
        "CGO_ENABLED": "0",
      },
      "hideSystemGoroutines": true,
    },
    {
      // https://github.com/golang/vscode-go/blob/master/docs/debugging.md#launch
      // The binary must be built with go build -gcflags=all="-N -l" to disable
      // inlining and optimizations that can interfere with debugging.
      "name": "üîé Attach to Go",
      "type": "go",
      "request": "attach",
      "mode": "local",
      "processId": 0,
    },
  ]
}