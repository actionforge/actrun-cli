    inputs:
      script: |
        mkdir -p "$DIST_NEW"


        if [[ "$TARGET_TYPE" == "cli" ]]; then
            if [[ "$OS" == "linux" || "$OS" == "macos" ]]; then
                # Copy files with matching $ARCH and no suffix on linux and macos
                find "$BUILD_DIR"/ -name "*$ARCH" -type f -not -name "*.*" -exec mv {} "$DIST_NEW/actrun" \;
            elif [[ "$OS" == "windows" ]]; then
                # Copy files with matching $ARCH and .exe suffix on win
                find "$BUILD_DIR"/ -name "*$ARCH.exe" -type f -exec mv {} "$DIST_NEW/actrun.exe" \;
            else
                echo "Unsupported OS: $OS"
                exit 1
            fi
        elif [[ "$TARGET_TYPE" == "py" ]]; then
            if [[ "$OS" == "linux" || "$OS" == "macos" ]]; then
                # Copy .so files with matching $ARCH on linux and macos
                find "$BUILD_DIR"/ -name "*$ARCH*.so" -type f -exec mv {} "$DIST_NEW/" \;
            elif [[ "$OS" == "windows" ]]; then
                # Copy .dll files with matching $ARCH on win
                find "$BUILD_DIR"/ -name "*$ARCH*.dll" -type f -exec mv {} "$DIST_NEW/" \;
            else
                echo "Unsupported OS: $OS"
                exit 1
            fi
        else
            echo "Unsupported TARGET_TYPE: $TARGET_TYPE"
            exit 1
        fi

        echo "Splitting $BUILD_DIR into $DIST_NEW done"
    comment: split dist/ into dist_cli_*/ and dist_py_*/
