            # Add Python libraries
            cp -r api/python/** $DIST_NEW
            
            case "$OS" in
                linux | macos) mv $DIST_NEW/*.so $DIST_NEW/actrun ;;
                windows) mv $DIST_NEW/*.dll $DIST_NEW/actrun ;;
                *) echo "Unsupported OS for library files: $OS" && exit 1 ;;
            esac

            # clean up Python cache files
            find $DIST_NEW -name "__pycache__" -exec rm -rf {} +
            find $DIST_NEW -name "*.pyc" -exec rm -f {} +

        elif [[ "$TARGET_TYPE" == "cli" ]]; then
            # set readme suffix based on OS type
            case "$OS" in
                linux | macos) README_SUFFIX="-nix" ;;
                windows) README_SUFFIX="-windows" ;;
                *)
                    echo "Unsupported OS: $OS"
                    exit 1
                    ;;
            esac
        else
            README_SUFFIX=""
        fi

        # copy readme and hello-world.act files
        cp extras/$TARGET_TYPE/readme$README_SUFFIX.md $DIST_NEW/readme.md
        cp extras/$TARGET_TYPE/hello-world.act $DIST_NEW/hello-world.act
