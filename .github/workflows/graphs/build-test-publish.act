    inputs:
      script: >
        if [[ -z "$CURRENT_OS" || -z "$ARCH" || -z "$OUTPUT_DIR" ]]; then
            echo "CURRENT_OS, ARCH and OUTPUT_DIR must be set."
            echo "CURRENT_OS: $CURRENT_OS"
            echo "ARCH: $ARCH"
            echo "OUTPUT_DIR: $OUTPUT_DIR"
            exit 1
        fi


        export CC=gcc

        # Cgo requires a cross-compiler that is not installed on GitHub runners
        as of now.

        [[ "$CURRENT_OS" == "linux" && "$ARCH" == "arm64" ]] &&
        CC=aarch64-linux-gnu-gcc


        build_cli() {
            OUTPUT_CLI="$OUTPUT_DIR/actrun-cli-$CURRENT_OS-$ARCH"
            [[ "$CURRENT_OS" == "windows" ]] && OUTPUT_CLI="$OUTPUT_CLI.exe"
            BUILD_FLAGS="-X actionforge/actrun-cli/build.Production=true -X actionforge/actrun-cli/build.License=$LICENSE -X actionforge/actrun-cli/build.Version=$GITHUB_REF_NAME"

            # Run Go tests if target matches the current system
            [[ "$CURRENT_OS" == "$CURRENT_OS" && "$ARCH" == "$CURRENT_ARCH" ]] && eval "go test -ldflags=\"$BUILD_FLAGS\" -o \"$OUTPUT_CLI\" ."

            echo "Build CLI for $CURRENT_OS-$ARCH"
            CGO_ENABLED=0 go build -ldflags="$BUILD_FLAGS" -o "$OUTPUT_CLI" .
        }


        build_lib() {
            if [[ "$CURRENT_OS" == "windows" && "$ARCH" == "arm64" ]]; then
                echo "Skip build. Cross-compilation for Windows ARM64 is not supported since I haven't found
                GCC for Windows that can cross compile to ARM64."
                return 0
            fi

            OUTPUT_DLL="$OUTPUT_DIR/actrun-py-$CURRENT_OS-$ARCH"
            case "$CURRENT_OS" in
                windows) OUTPUT_DLL="$OUTPUT_DLL.dll" ;;
                macos)   OUTPUT_DLL="$OUTPUT_DLL.so" ;;
                *)       OUTPUT_DLL="$OUTPUT_DLL.so" ;;
            esac
            echo "Build library for $CURRENT_OS-$ARCH"
            CGO_ENABLED=1 go build -tags="api,cpython" -buildmode=c-shared -ldflags="$BUILD_FLAGS" -o "$OUTPUT_DLL" .
        }


        # Run e2e tests for free license on matching CURRENT_OS and architecture

        run_e2e_tests() {
            if [[ "$LICENSE" == "free" ]]; then

                if [[ "$OS" == "$CURRENT_OS" && "$ARCH" == "$CURRENT_ARCH" ]]; then
                    TMP_ACTRUN="$(dirname $OUTPUT_CLI)/actrun"
                    [[ "$CURRENT_OS" == "windows" ]] && TMP_ACTRUN="$(dirname $OUTPUT_CLI)/actrun.exe"
                    cp "$OUTPUT_CLI" "$TMP_ACTRUN"
                    # encoding needed since default on Windows is cp1252
                    PYTHONIOENCODING=utf-8 python tests_e2e/tests_e2e.py
                    rm "$TMP_ACTRUN"
                else
                    echo "OS and ARCH do not match the current system ($OS != $CURRENT_OS $ARCH != $CURRENT_ARCH). Skipping e2e tests."
                fi
            else
                echo "Skipping e2e tests for pro license."
            fi
        }


        export GOOS=$CURRENT_OS

        export GOARCH=$ARCH


        # Adjust GOOS and GOARCH for specific cases

        [[ "$GOOS" = "macos" ]] && GOOS=darwin

        [[ "$GOARCH" = "x64" ]] && GOARCH=amd64


        build_cli

        build_lib

        run_e2e_tests
      env: []
