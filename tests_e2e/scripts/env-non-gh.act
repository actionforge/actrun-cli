        if [ -n "${GITHUB_ENV}" ]; then
            echo "ğŸš¨ Check failed, 'GITHUB_ENV' is defined but shouldn't"
            exit 1
        else
            echo "ğŸ‘ Check successful, 'GITHUB_ENV' is not defined"
