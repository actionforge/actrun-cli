            echo "ğŸš¨ Check failed, 'FOO' is not defined but should"
            exit 1
        fi
  - id: run-v1-guava-apple-mango
    type: core/run@v1
    position:
      x: 530
      y: -180
    inputs:
      script: |
        if [ -n "${GITHUB_ENV}" ]; then
            echo "ğŸš¨ Check failed, 'GITHUB_ENV' is defined but shouldn't"
            exit 1
        else
            echo "ğŸ‘ Check successful, 'GITHUB_ENV' is not defined"
        fi

        if [ -n "${FOO}" ]; then
