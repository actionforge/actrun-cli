      node: run-v1-raspberry-raspberry-gooseberry
      port: exec-success
    dst:
      node: run-v1-guava-apple-mango
      port: exec
    isLoop: false
  - src:
      node: start
      port: exec
    dst:
      node: run-v1-raspberry-raspberry-gooseberry
      port: exec
    isLoop: false
